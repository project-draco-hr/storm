{
  String setterName=toSetterName(property);
  Method retval=null;
  Method[] methods=clazz.getMethods();
  for (  Method method : methods) {
    if (setterName.equals(method.getName())) {
      LOG.info("Found setter method: " + method.getName());
      retval=method;
    }
  }
  return retval;
}
