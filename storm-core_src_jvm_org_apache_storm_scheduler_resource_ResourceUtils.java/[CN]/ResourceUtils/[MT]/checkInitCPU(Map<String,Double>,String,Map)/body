{
  if (!topology_resources.containsKey(Config.TOPOLOGY_COMPONENT_CPU_PCORE_PERCENT)) {
    topology_resources.put(Config.TOPOLOGY_COMPONENT_CPU_PCORE_PERCENT,org.apache.storm.utils.Utils.getDouble(topologyConf.get(Config.TOPOLOGY_COMPONENT_CPU_PCORE_PERCENT),null));
    debugMessage("CPU",Com,topologyConf);
  }
}
