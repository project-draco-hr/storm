{
  Yaml yaml=yaml();
  InputStream in=FluxParser.class.getResourceAsStream(resource);
  TopologyDef topology=loadYaml(yaml,in);
  in.close();
  if (dumpYaml) {
    dumpYaml(topology,yaml);
  }
  if (processIncludes) {
    return processIncludes(yaml,topology);
  }
 else {
    return topology;
  }
}
