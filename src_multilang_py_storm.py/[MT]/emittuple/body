def emittuple(tup, stream=None, anchors=[], directTask=None):
    global ANCHOR_TUPLE
    if (ANCHOR_TUPLE is not None):
        anchors = [ANCHOR_TUPLE]
    m = {'command': 'emit', }
    if (stream is not None):
        m['stream'] = stream
    m['anchors'] = map((lambda a: a.id), anchors)
    if (directTask is not None):
        m['task'] = directTask
    m['tuple'] = tup
    sendMsgToParent(m)
