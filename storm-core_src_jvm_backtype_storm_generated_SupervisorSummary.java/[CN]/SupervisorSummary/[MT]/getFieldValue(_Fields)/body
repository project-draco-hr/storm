{
switch (field) {
case HOST:
    return get_host();
case UPTIME_SECS:
  return get_uptime_secs();
case NUM_WORKERS:
return get_num_workers();
case NUM_USED_WORKERS:
return get_num_used_workers();
case SUPERVISOR_ID:
return get_supervisor_id();
case VERSION:
return get_version();
case TOTAL_RESOURCES:
return get_total_resources();
}
throw new IllegalStateException();
}
