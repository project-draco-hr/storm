{
  boolean publishNow=consumerStartedFlag;
  if (!publishNow) {
    cacheReadLock.lock();
    try {
      publishNow=consumerStartedFlag;
      if (!publishNow) {
        _cache.add(obj);
      }
    }
  finally {
      cacheReadLock.unlock();
    }
  }
  if (publishNow) {
    publishDirect(obj,block);
  }
}
