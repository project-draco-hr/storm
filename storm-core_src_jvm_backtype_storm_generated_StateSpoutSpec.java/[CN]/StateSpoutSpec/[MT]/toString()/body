{
  StringBuilder sb=new StringBuilder("StateSpoutSpec(");
  boolean first=true;
  sb.append("state_spout_object:");
  if (this.state_spout_object == null) {
    sb.append("null");
  }
 else {
    sb.append(this.state_spout_object);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("common:");
  if (this.common == null) {
    sb.append("null");
  }
 else {
    sb.append(this.common);
  }
  first=false;
  sb.append(")");
