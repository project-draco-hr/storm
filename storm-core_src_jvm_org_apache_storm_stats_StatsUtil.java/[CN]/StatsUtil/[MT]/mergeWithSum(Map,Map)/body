{
  Map ret=new HashMap();
  Set<Object> allKeys=new HashSet<>();
  if (m1 != null) {
    allKeys.addAll(m1.keySet());
  }
  if (m2 != null) {
    allKeys.addAll(m2.keySet());
  }
  for (  Object k : allKeys) {
    Number n1=getOr0(m1,k);
    Number n2=getOr0(m2,k);
    ret.put(k,add(n1,n2));
  }
  return ret;
}
