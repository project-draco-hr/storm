{
  double ret=0;
  if (id2Avg == null || id2num == null) {
    return ret;
  }
  for (  Object o : id2Avg.entrySet()) {
    Map.Entry entry=(Map.Entry)o;
    Object k=entry.getKey();
    double v=((Number)entry.getValue()).doubleValue();
    long n=((Number)id2num.get(k)).longValue();
    ret+=productOr0(v,n);
  }
  return ret;
}
