{
switch (field) {
case SUPERVISORS:
    if (value == null) {
      unset_supervisors();
    }
 else {
      set_supervisors((List<SupervisorSummary>)value);
    }
  break;
case TOPOLOGIES:
if (value == null) {
  unset_topologies();
}
 else {
  set_topologies((List<TopologySummary>)value);
}
break;
case NIMBUSES:
if (value == null) {
unset_nimbuses();
}
 else {
set_nimbuses((List<NimbusSummary>)value);
}
break;
}
}
