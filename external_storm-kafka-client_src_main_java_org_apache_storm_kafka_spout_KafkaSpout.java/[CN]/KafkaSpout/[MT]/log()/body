{
switch (pollStrategy) {
case STREAM:
    LOG.trace("Reached the maximum number number of uncommitted records [{}]. " + "No more polls will occur until a sequence of commits sets the count under the [{}] threshold ",numUncommittedOffsets,kafkaSpoutConfig.getMaxUncommittedOffsets());
  break;
case BATCH:
LOG.trace("No more polls will occur until the last batch completes. [{}] emitted tuples pending",numUncommittedOffsets);
break;
default :
throw new IllegalStateException("No implementation defined for polling strategy " + pollStrategy);
}
}
