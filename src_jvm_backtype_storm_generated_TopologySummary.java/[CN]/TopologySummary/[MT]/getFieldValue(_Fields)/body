{
switch (field) {
case ID:
    return get_id();
case NAME:
  return get_name();
case NUM_TASKS:
return Integer.valueOf(get_num_tasks());
case NUM_WORKERS:
return Integer.valueOf(get_num_workers());
case UPTIME_SECS:
return Integer.valueOf(get_uptime_secs());
case STATUS:
return get_status();
}
throw new IllegalStateException();
}
