{
  StringBuilder sb=new StringBuilder("NodeInfo(");
  boolean first=true;
  sb.append("node:");
  if (this.node == null) {
    sb.append("null");
  }
 else {
    sb.append(this.node);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("port:");
  if (this.port == null) {
    sb.append("null");
  }
 else {
    sb.append(this.port);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
