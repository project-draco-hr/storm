{
switch (field) {
case E:
    if (value == null) {
      unset_e();
    }
 else {
      set_e((NotAliveException)value);
    }
  break;
case ITE:
if (value == null) {
  unset_ite();
}
 else {
  set_ite((InvalidTopologyException)value);
}
break;
case AZE:
if (value == null) {
unset_aze();
}
 else {
set_aze((AuthorizationException)value);
}
break;
}
}
