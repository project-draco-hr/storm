{
switch (field) {
case ID:
    if (value == null) {
      unset_id();
    }
 else {
      set_id((String)value);
    }
  break;
case HOST:
if (value == null) {
  unset_host();
}
 else {
  set_host((String)value);
}
break;
case IS_INCLUDE_SYS:
if (value == null) {
unset_is_include_sys();
}
 else {
set_is_include_sys((Boolean)value);
}
break;
}
}
