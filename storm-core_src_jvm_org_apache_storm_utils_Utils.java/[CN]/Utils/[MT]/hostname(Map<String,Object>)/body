{
  if (conf == null) {
    return memoizedLocalHostname();
  }
  Object hostnameString=conf.get(Config.STORM_LOCAL_HOSTNAME);
  if (hostnameString == null) {
    return memoizedLocalHostname();
  }
  if (hostnameString.equals("")) {
    return memoizedLocalHostname();
  }
  return hostnameString.toString();
}
