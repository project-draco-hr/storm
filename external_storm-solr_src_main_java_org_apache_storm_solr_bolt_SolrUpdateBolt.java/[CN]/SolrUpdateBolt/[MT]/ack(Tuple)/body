{
  if (commitStgy == null) {
    collector.ack(tuple);
  }
 else {
    boolean forceCommit=false;
    if (TupleUtils.isTick(tuple)) {
      LOG.debug("TICK! forcing solr client commit");
      collector.ack(tuple);
      forceCommit=true;
    }
 else {
      toCommitTuples.add(tuple);
      commitStgy.update();
    }
    if (forceCommit || commitStgy.commit()) {
      solrClient.commit(solrMapper.getCollection());
      ackCommittedTuples();
    }
  }
}
