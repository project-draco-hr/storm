{
  if (commitStgy == null) {
    collector.ack(tuple);
  }
 else {
    if (TupleUtils.isTick(tuple)) {
      LOG.debug("TICK! forcing solr client commit");
      collector.ack(tuple);
      commitStgy.commit();
      solrClient.commit(solrMapper.getCollection());
      ackCommittedTuples();
    }
 else {
      toCommitTuples.add(tuple);
      commitStgy.update();
      if (commitStgy.commit()) {
        solrClient.commit(solrMapper.getCollection());
        ackCommittedTuples();
      }
    }
  }
}
