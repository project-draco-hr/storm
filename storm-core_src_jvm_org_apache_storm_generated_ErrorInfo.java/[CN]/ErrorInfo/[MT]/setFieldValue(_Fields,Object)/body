{
switch (field) {
case ERROR:
    if (value == null) {
      unset_error();
    }
 else {
      set_error((String)value);
    }
  break;
case ERROR_TIME_SECS:
if (value == null) {
  unset_error_time_secs();
}
 else {
  set_error_time_secs((Integer)value);
}
break;
case HOST:
if (value == null) {
unset_host();
}
 else {
set_host((String)value);
}
break;
case PORT:
if (value == null) {
unset_port();
}
 else {
set_port((Integer)value);
}
break;
}
}
