{
  return new IOpaquePartitionedTridentSpout.Emitter<GlobalPartitionInformation,GlobalPartitionId,Map>(){
    @Override public Map emitPartitionBatch(    TransactionAttempt transactionAttempt,    TridentCollector tridentCollector,    GlobalPartitionId globalPartitionId,    Map map){
      return emitNewPartitionBatch(transactionAttempt,tridentCollector,globalPartitionId,map);
    }
    @Override public void refreshPartitions(    List<GlobalPartitionId> globalPartitionIds){
      refresh(globalPartitionIds);
    }
    @Override public List<GlobalPartitionId> getOrderedPartitions(    GlobalPartitionInformation partitionInformation){
      return orderPartitions(partitionInformation);
    }
    @Override public void close(){
      clear();
    }
  }
;
}
