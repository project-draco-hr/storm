{
switch (field) {
case INPUTS:
    if (value == null) {
      unset_inputs();
    }
 else {
      set_inputs((Map<GlobalStreamId,Grouping>)value);
    }
  break;
case STREAMS:
if (value == null) {
  unset_streams();
}
 else {
  set_streams((Map<String,StreamInfo>)value);
}
break;
case PARALLELISM_HINT:
if (value == null) {
unset_parallelism_hint();
}
 else {
set_parallelism_hint((Integer)value);
}
break;
}
}
