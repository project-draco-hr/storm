{
  StringBuilder sb=new StringBuilder("ComponentCommon(");
  boolean first=true;
  sb.append("streams:");
  if (this.streams == null) {
    sb.append("null");
  }
 else {
    sb.append(this.streams);
  }
  first=false;
  if (is_set_parallelism_hint()) {
    if (!first)     sb.append(", ");
    sb.append("parallelism_hint:");
    sb.append(this.parallelism_hint);
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
