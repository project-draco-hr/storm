{
switch (field) {
case TIME_SECS:
    return Long.valueOf(get_time_secs());
case HOSTNAME:
  return get_hostname();
case ASSIGNMENT_ID:
return get_assignment_id();
case USED_PORTS:
return get_used_ports();
case META:
return get_meta();
case SCHEDULER_META:
return get_scheduler_meta();
case UPTIME_SECS:
return Long.valueOf(get_uptime_secs());
}
throw new IllegalStateException();
}
