{
switch (field) {
case EXECUTOR_INFO:
    return get_executor_info();
case COMPONENT_ID:
  return get_component_id();
case HOST:
return get_host();
case PORT:
return Integer.valueOf(get_port());
case UPTIME_SECS:
return Integer.valueOf(get_uptime_secs());
case STATS:
return get_stats();
}
throw new IllegalStateException();
}
