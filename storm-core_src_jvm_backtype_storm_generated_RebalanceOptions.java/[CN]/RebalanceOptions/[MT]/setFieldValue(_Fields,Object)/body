{
switch (field) {
case WAIT_SECS:
    if (value == null) {
      unset_wait_secs();
    }
 else {
      set_wait_secs((Integer)value);
    }
  break;
case NUM_WORKERS:
if (value == null) {
  unset_num_workers();
}
 else {
  set_num_workers((Integer)value);
}
break;
case NUM_EXECUTORS:
if (value == null) {
unset_num_executors();
}
 else {
set_num_executors((Map<String,Integer>)value);
}
break;
}
}
