{
  StringBuilder sb=new StringBuilder("GlobalStreamId(");
  boolean first=true;
  sb.append("componentId:");
  if (this.componentId == null) {
    sb.append("null");
  }
 else {
    sb.append(this.componentId);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("streamId:");
  if (this.streamId == null) {
    sb.append("null");
  }
 else {
    sb.append(this.streamId);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
