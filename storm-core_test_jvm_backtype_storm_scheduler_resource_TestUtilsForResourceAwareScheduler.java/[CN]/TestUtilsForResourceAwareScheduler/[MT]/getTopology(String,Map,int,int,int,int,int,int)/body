{
  Config conf=new Config();
  conf.putAll(config);
  conf.put(Config.TOPOLOGY_PRIORITY,priority);
  conf.put(Config.TOPOLOGY_NAME,name);
  StormTopology topology=buildTopology(numSpout,numBolt,spoutParallelism,boltParallelism);
  TopologyDetails topo=new TopologyDetails(name + "-" + launchTime,conf,topology,0,genExecsAndComps(topology,spoutParallelism,boltParallelism),launchTime);
  return topo;
}
