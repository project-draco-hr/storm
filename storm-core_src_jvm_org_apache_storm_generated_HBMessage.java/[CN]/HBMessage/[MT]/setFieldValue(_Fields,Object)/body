{
switch (field) {
case TYPE:
    if (value == null) {
      unset_type();
    }
 else {
      set_type((HBServerMessageType)value);
    }
  break;
case DATA:
if (value == null) {
  unset_data();
}
 else {
  set_data((HBMessageData)value);
}
break;
case MESSAGE_ID:
if (value == null) {
unset_message_id();
}
 else {
set_message_id((Integer)value);
}
break;
}
}
