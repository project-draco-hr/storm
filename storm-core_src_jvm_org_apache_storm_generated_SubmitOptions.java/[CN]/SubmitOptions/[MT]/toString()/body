{
  StringBuilder sb=new StringBuilder("SubmitOptions(");
  boolean first=true;
  sb.append("initial_status:");
  if (this.initial_status == null) {
    sb.append("null");
  }
 else {
    sb.append(this.initial_status);
  }
  first=false;
  if (is_set_creds()) {
    if (!first)     sb.append(", ");
    sb.append("creds:");
    if (this.creds == null) {
      sb.append("null");
    }
 else {
      sb.append(this.creds);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
