{
switch (field) {
case ID:
    if (value == null) {
      unset_id();
    }
 else {
      set_id((String)value);
    }
  break;
case COMPONENT_ID:
if (value == null) {
  unset_component_id();
}
 else {
  set_component_id((String)value);
}
break;
case ACTION:
if (value == null) {
unset_action();
}
 else {
set_action((ProfileAction)value);
}
break;
}
}
