{
switch (field) {
case SUCCESS:
    if (value == null) {
      unset_success();
    }
 else {
      set_success((StormTopology)value);
    }
  break;
case E:
if (value == null) {
  unset_e();
}
 else {
  set_e((NotAliveException)value);
}
break;
case AZE:
if (value == null) {
unset_aze();
}
 else {
set_aze((AuthorizationException)value);
}
break;
}
}
