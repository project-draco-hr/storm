{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.is_set_id()) {
    optionals.set(0);
  }
  if (struct.is_set_options()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.is_set_id()) {
    oprot.writeString(struct.id);
  }
  if (struct.is_set_options()) {
    struct.options.write(oprot);
  }
}
