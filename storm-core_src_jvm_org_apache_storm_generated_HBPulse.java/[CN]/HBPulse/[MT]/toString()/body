{
  StringBuilder sb=new StringBuilder("HBPulse(");
  boolean first=true;
  sb.append("id:");
  if (this.id == null) {
    sb.append("null");
  }
 else {
    sb.append(this.id);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("details:");
  if (this.details == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.details,sb);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
