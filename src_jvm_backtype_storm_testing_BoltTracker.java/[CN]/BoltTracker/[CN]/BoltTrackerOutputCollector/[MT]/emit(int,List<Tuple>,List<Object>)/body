{
  List<Integer> ret=_collector.emit(streamId,anchors,tuple);
  transferred+=ret.size();
  if (ret.size() != 1) {
    Set<Long> anchorIds=new HashSet<Long>();
    for (    Tuple t : anchors) {
      anchorIds.addAll(t.getMessageId().getAnchors());
    }
    transferred+=anchorIds.size();
  }
  return ret;
}
