{
switch (field) {
case SPOUTS:
    return get_spouts();
case BOLTS:
  return get_bolts();
case STATE_SPOUTS:
return get_state_spouts();
case WORKER_HOOKS:
return get_worker_hooks();
case DEPENDENCY_JARS:
return get_dependency_jars();
case DEPENDENCY_ARTIFACTS:
return get_dependency_artifacts();
}
throw new IllegalStateException();
}
