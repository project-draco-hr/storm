{
  LOG.info("Preparing HDFS Bolt...");
  super.prepare(conf,topologyContext,collector);
  try {
    this.fs=FileSystem.get(URI.create(this.fsUrl),hdfsConfig);
    createOutputFile();
  }
 catch (  Exception e) {
    throw new RuntimeException("Error preparing HdfsBolt: " + e.getMessage(),e);
  }
}
