{
  if (topo1.getTopologyPriority() > topo2.getTopologyPriority()) {
    return 1;
  }
 else   if (topo1.getTopologyPriority() < topo2.getTopologyPriority()) {
    return -1;
  }
 else {
    if (topo1.getUpTime() > topo2.getUpTime()) {
      return -1;
    }
 else     if (topo1.getUpTime() < topo2.getUpTime()) {
      return 1;
    }
 else {
      return 0;
    }
  }
}
