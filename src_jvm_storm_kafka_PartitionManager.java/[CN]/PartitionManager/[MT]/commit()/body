{
  LOG.info("Committing offset for " + _partition);
  long committedTo;
  if (_pending.isEmpty()) {
    committedTo=_emittedToOffset;
  }
 else {
    committedTo=_pending.first();
  }
  if (committedTo != _committedTo) {
    LOG.info("Writing committed offset to ZK: " + committedTo);
    Map<Object,Object> data=(Map<Object,Object>)ImmutableMap.builder().put("topology",ImmutableMap.of("id",_topologyInstanceId,"name",_stormConf.get(Config.TOPOLOGY_NAME))).put("offset",committedTo).put("partition",_partition.partition).put("broker",ImmutableMap.of("host",_partition.host.host,"port",_partition.host.port)).put("topic",_spoutConfig.topic).build();
    _state.writeJSON(committedPath(),data);
    LOG.info("Wrote committed offset to ZK: " + committedTo);
    _committedTo=committedTo;
  }
  LOG.info("Comitted offset for " + _partition);
}
