{
  LOG.info("[req " + context.requestID() + "] Access "+ " from: "+ (context.remoteAddress() == null ? "null" : context.remoteAddress().toString())+ " principal:"+ (context.principal() == null ? "null" : context.principal())+ " op:"+ operation+ " topoology:"+ topology_conf.get(Config.TOPOLOGY_NAME));
  return true;
}
