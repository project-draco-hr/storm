{
switch (field) {
case NUM_EXECUTORS:
    return get_num_executors();
case NUM_TASKS:
  return get_num_tasks();
case EMITTED:
return get_emitted();
case TRANSFERRED:
return get_transferred();
case ACKED:
return get_acked();
case FAILED:
return get_failed();
case RESOURCES_MAP:
return get_resources_map();
}
throw new IllegalStateException();
}
