{
  if (input != null) {
    Pattern optsPattern=Pattern.compile("Xmx[0-9]+m");
    Matcher m=optsPattern.matcher(input);
    String memoryOpts=null;
    while (m.find()) {
      memoryOpts=m.group();
    }
    if (memoryOpts != null) {
      memoryOpts=memoryOpts.replaceAll("[a-zA-Z]","");
      return Double.parseDouble(memoryOpts);
    }
 else {
      return defaultValue;
    }
  }
 else {
    return defaultValue;
  }
}
