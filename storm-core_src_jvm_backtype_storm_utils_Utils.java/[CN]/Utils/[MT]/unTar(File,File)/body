{
  if (!untarDir.mkdirs()) {
    if (!untarDir.isDirectory()) {
      throw new IOException("Mkdirs failed to create " + untarDir);
    }
  }
  boolean gzipped=inFile.toString().endsWith("gz");
  if (onWindows()) {
    unTarUsingJava(inFile,untarDir,gzipped);
  }
 else {
    unTarUsingTar(inFile,untarDir,gzipped);
  }
}
