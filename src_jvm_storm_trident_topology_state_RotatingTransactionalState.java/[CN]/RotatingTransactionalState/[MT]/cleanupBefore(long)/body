{
  SortedMap<Long,Object> toDelete=_curr.headMap(txid);
  for (  long tx : new HashSet<Long>(toDelete.keySet())) {
    _curr.remove(tx);
    try {
      _state.delete(txPath(tx));
    }
 catch (    RuntimeException e) {
      if (!(e.getCause() instanceof KeeperException.NoNodeException)) {
        throw e;
      }
    }
  }
}
