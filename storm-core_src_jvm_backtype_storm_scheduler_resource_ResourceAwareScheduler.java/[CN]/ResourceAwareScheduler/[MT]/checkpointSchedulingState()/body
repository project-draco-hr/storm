{
  LOG.info("checkpointing scheduling state...");
  LOG.info("/*********Checkpoint************/");
  for (  User user : this.getUserMap().values()) {
    LOG.info(user.getDetailedInfo());
  }
  LOG.info("/*********End************/");
  return new SchedulingState(this.userMap,this.cluster,this.topologies,this.nodes,this.conf);
}
